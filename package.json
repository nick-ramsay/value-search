{
  "name": "value-search",
  "version": "1.0.0",
  "description": "A template used as a starter application for create MERN applications",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nick-ramsay/value-search.git"
  },
  "author": "Nick Ramsay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nick-ramsay/value-search/issues"
  },
  "homepage": "https://github.com/nick-ramsay/value-search#readme",
  "dependencies": {
    "axios": "^1.2.1",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "dd-trace": "^3.10.0",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "googleapis": "^109.0.1",
    "if-env": "^1.0.4",
    "js-sha256": "^0.9.0",
    "mongodb": "^3.6.2",
    "mongoose": "^6.7.2",
    "mongoose-findorcreate": "^3.0.0",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.4",
    "sha256": "^0.2.0"
  }
}
